{
	"name": "@surveyplanet/svelte-components",
	"description": "A collection of Svelte UI components",
	"keywords": [
		"svelte",
		"component",
		"avatar",
		"badge",
		"breadcrumbs",
		"button",
		"checkbox",
		"icon",
		"radio",
		"toggle"
	],
	"version": "0.0.8",
	"homepage": "https://ui.surveyplanet.com",
	"license": "MIT",
	"author": "Jonah Werre <jonahwerre@gmail.com> (https://github.com/jwerre)",
	"contributors": [
		"Ognjen Ugljenovic <ognjen@surveyplanet.com>"
	],
	"repository": {
		"type": "git",
		"url": "git@github.com:surveyplanet/svelte_components.git"
	},
	"scripts": {
		"build": "svelte-kit sync && svelte-package",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"clean:hard": "rm package-lock.json && npm run clean",
		"clean": "rm -rf package coverage node_modules dist static .svelte-kit && npm i",
		"dev": "npm run storybook",
		"format": "prettier --plugin-search-dir . --write .",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"lint:check": "eslint .",
		"package": "npm run build",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"release": "semantic-release",
		"sb": "npm run storybook",
		"spell": "cspell",
		"storybook:build": "rm -rf static && storybook build --output-dir static",
		"storybook": "storybook dev -p 6006",
		"test": "npx playwright test",
		"test:ci": "echo 'TODO'",
		"proof": "npm run check && npm run lint && npm run spell && npm run test"
	},
	"devDependencies": {
		"@semantic-release/changelog": "^6.0.2",
		"@playwright/test": "^1.30.0",
		"@semantic-release/commit-analyzer": "^9.0.2",
		"@semantic-release/github": "^8.0.7",
		"@semantic-release/npm": "^9.0.2",
		"@semantic-release/release-notes-generator": "^10.0.3",
		"@storybook/addon-a11y": "^7.0.0-beta.30",
		"@storybook/addon-actions": "^7.0.0-beta.30",
		"@storybook/addon-docs": "^7.0.0-beta.30",
		"@storybook/addon-essentials": "^7.0.0-beta.30",
		"@storybook/addon-interactions": "^7.0.0-beta.30",
		"@storybook/addon-links": "^7.0.0-beta.30",
		"@storybook/addon-svelte-csf": "^3.0.0-next.3",
		"@storybook/blocks": "^7.0.0-beta.30",
		"@storybook/client-api": "^7.0.0-beta.30",
		"@storybook/jest": "^0.0.10",
		"@storybook/svelte": "^7.0.0-beta.30",
		"@storybook/sveltekit": "^7.0.0-beta.30",
		"@storybook/test-runner": "^0.10.0-next.4",
		"@storybook/testing-library": "^0.0.13",
		"@surveyplanet/publish": "^1.0.1",
		"@surveyplanet/styles": "^0.1.7-25",
		"@sveltejs/adapter-auto": "^1.0.0-next.90",
		"@sveltejs/kit": "^1.0.0-next.587",
		"@sveltejs/package": "^1.0.0-next.6",
		"@types/cleave.js": "^1.4.7",
		"@typescript-eslint/eslint-plugin": "^5.51.0",
		"@typescript-eslint/parser": "^5.51.0",
		"concurrently": "^7.6.0",
		"cspell": "^6.22.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-storybook": "^0.6.8",
		"eslint-plugin-svelte3": "^4.0.0",
		"http-server": "^14.1.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"remark-gfm": "^3.0.1",
		"sass": "^1.57.0",
		"storybook": "^7.0.0-beta.30",
		"svelte": "^3.54.0",
		"svelte-check": "^2.10.3",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"wait-on": "^7.0.1"
	},
	"type": "module",
	"dependencies": {
		"@surveyplanet/utilities": "^1.0.0-beta.5",
		"cleave.js": "^1.6.0"
	}
}
